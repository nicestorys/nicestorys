{
  "name": "nicestorys",
  "private": true,
  "version": "0.0.0",
  "author": "NiceStorys",
  "type": "module",
  "scripts": {
    "dev": "concurrently --kill-others \"vite\" \"tsx watch --ignore 'vite.config.ts.timestamp-*.mjs' --ignore 'src/client/**/*' src/main.dev.ts\"",
    "check:format": "prettier --ignore-path .gitignore --check .",
    "check:lint": "eslint --ext .tsx,.ts --ignore-path .gitignore .",
    "test:e2e": "cypress run --browser chrome",
    "build": "vite build && tsup",
    "start": "node dist/main.js"
  },
  "engines": {
    "node": "^18.0.0 || >=20.0.0"
  },
  "dependencies": {
    "@fontsource-variable/inter": "^5.0.17",
    "@fontsource-variable/jetbrains-mono": "^5.0.20",
    "@iconify-icons/uil": "^1.2.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "hono": "^4.2.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.2",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.3"
  },
  "devDependencies": {
    "@hono/node-server": "^1.11.0",
    "@hono/swagger-ui": "^0.2.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "cypress": "^13.7.3",
    "esbuild": "^0.20.2",
    "eslint": "^8.57.0",
    "eslint-plugin-cypress": "^3.0.1",
    "fs-extra": "^11.2.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.3",
    "tsup": "^8.0.2",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.8"
  }
}
